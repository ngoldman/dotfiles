[color]
  ui = auto
[alias]
  a = add
  b = branch
  c = commit
  cm = commit -m
  d = diff
  dw = diff --word-diff
  ds = diff --staged
  dws = diff --word-diff --staged
  co = checkout
  r = remote
  s = status
  sup = branch --all --verbose
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  unstage = reset HEAD --
  done = branch --merged
  todo = branch --no-merged
  update = fetch --all --prune
  lastchange = log -p --follow -n 1
  praise = blame
[credential]
  helper = osxkeychain
[mergetool]
  keepBackup = true
[core]
  excludesfile = ~/.gitignore_global
  autocrlf = input
  ignorecase = false
  editor = code -nw
[push]
  default = simple
  autoSetupRemote = true
